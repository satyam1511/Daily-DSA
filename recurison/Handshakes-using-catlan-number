 int solve(int n )
    {
        if(n==0||n ==1)
        return 1;
        
        int sum = 0;
        for(int i = n - 1 ; i >= 0 ; i--)
        sum += solve(i) * solve(n - i- 1);
        
        return sum ;
    }
    
    
    int count(int N){
        // code here
        int n = N / 2;
        return solve(n);
    }
